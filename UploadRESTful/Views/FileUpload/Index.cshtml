<form method="post" enctype="multipart/form-data" asp-controller="FileUpload"
		asp-action="Index">
	<div class="form-group">
		<div class="col-md-10">
			<p>Загрузите файл для его обработки</p>
			<input type="file" name="files" />
		</div>
	</div>
	<div class="form-group">
		<div class="col-md-10">
			<input type="submit" />
		</div>
	</div>
</form>